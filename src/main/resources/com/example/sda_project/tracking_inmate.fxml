<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sda_project.TrackingInmateController">
    <children>
        <!-- "View Inmate Location" Button -->
        <Button fx:id="recordlocationButton" layoutX="187.0" layoutY="96.0" onAction="#recordbuttonclicked" prefHeight="87.0" prefWidth="226.0"
                style="-fx-background-color: #0F3057; -fx-text-fill: white;" text="View Inmate Location" />

        <!-- "Update Inmate Location" Button -->
        <Button fx:id="viewlocationButton" layoutX="187.0" layoutY="220.0" onAction="#viewbuttonclicked" prefHeight="87.0" prefWidth="226.0"
                style="-fx-background-color: #0F3057; -fx-text-fill: white;" text="Update Inmate Location" />

        <!-- "Back" Button -->
        <Button fx:id="backButton" layoutX="534.0" layoutY="21.0" onAction="#onBackButtonClick"
                style="-fx-background-color: white; -fx-text-fill: #0F3057;" text="Back" />
    </children>
</AnchorPane>
